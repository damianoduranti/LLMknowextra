VAR End : boolean;
VAR pc : 1..10;

ASSIGN
 init(pc) := 1;
 next(pc) := case
   pc <= 9 : pc + 1;
   TRUE    : 10;
   esac;
  init(End) := FALSE;
  next(End) := case
    pc >= 9 : TRUE;
    TRUE    : End;
  esac;

